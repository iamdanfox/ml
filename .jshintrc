{
  "bitwise": true,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "indent": 2,
  "latedef": true,
  "loopfunc": true,
  "newcap": true,
  "noarg": true,
  "node": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": true,
  "quotmark": "double",
  "strict": true,
  "undef": true,
  "unused": true,
  "maxcomplexity": 4,
  "maxdepth": 3,
